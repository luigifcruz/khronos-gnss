{
    "name": "khronos-frontend",
    "version": "1.0.0",
    "description": "Front-end for the Khronos Platform",
    "main": "index.js",
    "scripts": {
        "server": "babel-node server.prod.js",
        "dev": "webpack --config webpack.config.prod.js && npm run server",
        "build": "webpack --config webpack.config.prod.js && mkspiffs -c dist/ -b 4096 -p 256 -s 0x200000 spiffs.bin",
        "flash": "esptool.py --chip esp32 --port /dev/ttyUSB0 --baud 921600 write_flash -z 0x110000 spiffs.bin",
        "deploy": "npm run build && npm run flash"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "async": "^2.0.0",
        "cookie-parser": "^1.4.3",
        "css-loader": "^0.23.1",
        "ejs": "^2.5.2",
        "express": "^4.13.4",
        "google-fonts-webpack-plugin": "^0.4.4",
        "json-loader": "^0.5.4",
        "md5": "^2.1.0",
        "node-sass": "^3.7.0",
        "node-uuid": "^1.4.7",
        "react": "^15.0.2",
        "react-addons-css-transition-group": "^15.3.2",
        "react-cookie": "^0.4.8",
        "react-dom": "^15.0.2",
        "react-redux": "^4.4.5",
        "react-router": "^2.4.1",
        "redux": "^3.5.2",
        "redux-logger": "^2.7.0",
        "sass-loader": "^3.2.0",
        "style-loader": "^0.13.1",
        "superagent": "^2.0.0-alpha.3"
    },
    "devDependencies": {
        "babel-cli": "^6.11.4",
        "babel-core": "^6.13.2",
        "babel-loader": "^6.2.4",
        "babel-plugin-transform-require-ignore": "0.0.2",
        "babel-preset-es2015": "^6.13.2",
        "babel-preset-react": "^6.11.1",
        "babel-preset-react-hmre": "^1.1.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "css-loader": "^0.23.1",
        "webpack": "^1.15.0",
        "webpack-cli": "^2.1.3"
    }
}
