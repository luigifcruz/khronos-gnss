{
    "name": "khronos-frontend",
    "version": "1.0.0",
    "description": "Front-end for the Khronos Platform",
    "main": "index.js",
    "scripts": {
        "server": "node server.prod.js",
        "dev": "webpack --config webpack.config.dev.js && npm run server",
        "build": "webpack --config webpack.config.prod.js && mkspiffs -c dist/ -b 4096 -p 256 -s 0x200000 spiffs.bin",
        "flash": "esptool.py --chip esp32 --port /dev/ttyUSB0 --baud 921600 write_flash -z 0x110000 spiffs.bin",
        "deploy": "npm run build && npm run flash"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "async": "^2.0.0",
        "cookie-parser": "^1.4.3",
        "ejs": "^2.5.2",
        "express": "^4.13.4",
        "json-loader": "^0.5.4",
        "md5": "^2.1.0",
        "node-uuid": "^1.4.7",
        "pondjs": "^0.8.9",
        "react": "^15.0.2",
        "react-addons-css-transition-group": "^15.3.2",
        "react-cookie": "^0.4.8",
        "react-dom": "^15.0.2",
        "react-redux": "^4.4.5",
        "react-router": "^2.4.1",
        "react-timeseries-charts": "^0.15.3",
        "react-websocket": "^2.0.0",
        "redux": "^3.5.2",
        "redux-logger": "^2.7.0",
        "style-loader": "^0.13.1",
        "superagent": "^2.0.0-alpha.3"
    },
    "devDependencies": {
        "@babel/core": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.2",
        "compression-webpack-plugin": "^2.0.0",
        "css-loader": "^0.23.1",
        "del-webpack-plugin": "^1.1.0",
        "mini-css-extract-plugin": "^0.4.3",
        "node-sass": "^4.9.3",
        "sass-loader": "^7.1.0",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.19.1",
        "webpack-bundle-analyzer": "^3.0.2",
        "webpack-cli": "^2.1.3"
    }
}
